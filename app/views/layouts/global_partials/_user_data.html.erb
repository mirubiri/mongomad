<aside class="user_container_profile" id="user_data_container">
  <div class="image">
    <%= user_visited_image %>
  </div>
  <div class="user_name">
    <%= user_visited_nickname %></br>
    <%= user_visited_country %></br>
  </div> 

  <% if user_logged_id != user_visited_id %> 
    <div class="new_offer_button">     
      <%= link_to "Hazme una nueva oferta",
          new_user_offer_path(current_user),
          :remote => true %> 
    </div>      
  <% end %>

  <nav class="profile_menu" id="menu_user_data">
    <div class="menu_icon">
      <%= link_to "Perfil", user_profile_path(@user), :remote => true, :id => "link_profile" %>
    </div>
    <div class="menu_icon">
      <%= link_to 'Prod', user_things_path(@user), :remote => true, :id => "link_things" %>
    </div>
    <div class="menu_icon">
      <%= link_to 'Alertas'%>
    </div>    
  </nav>
</aside>