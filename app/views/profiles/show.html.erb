<div class="profile_complete">
  <div class="person_info">
    <div class="title">
      Personal Info
    </div>
    <div class="info_side">
      <div class="info_element">
        <div class="info_element_title">Name & Birthdate:</div>
        <div class="info_element_description">
          <%= user_fullname(@user) %>,
          <%= user_birthdate(@user) %>
        </div>
      </div>
      <div class="info_element">
        <div class="info_element_title">Direction:</div>
        <div class="info_element_description">
          <%#= user_country(@user) %>
        </div>
      </div>
      <div class="info_element">
        <div class="info_element_title">E-Mail:</div>
        <div class="info_element_description">
          <%= user_email(@user) %>
        </div>
      </div>
      <div class="info_element">
        <div class="info_element_title">Phones:</div>
        <div class="info_element_description">
          <%#= user_phone_number(@user) %>
        </div>
      </div>
      <div class="info_element">
        <div class="info_element_title">Web:</div>
        <div class="info_element_description">
          <%#= user_website(@user) %>
        </div>
      </div>
    </div>
    <div class="image_side">
        <%= user_image(@user) %>
        <% if id(@user) == current_user.id %>
          <%= link_to "editar mi perfil",
              edit_user_profile_path(current_user),
              :remote => true,
              :class => "btn btn-warning" %>
        <% end %>
    </div>
  </div>
  <div class="statistics">
    <div class="title">Times Table</div>
    <div class="result_side">
      <div class="result_title">Deals as buyer</div>
      <div class="result_number">10</div>
      <div class="result_title">Deals as seller</div>
      <div class="result_number">50</div>
      <div class="result_title">Total use time</div>
      <div class="result_number">500h</div>
      <div class="result_title">Dayle use average</div>
      <div class="result_number">2h</div>
    </div>
    <div class="graphique_side">
      <div class="graphique"></div>
    </div>
  </div>
  <div class="comments_recieved">
    <div class="title">Comments</div>
    <div id="data_list">
      <div class="data_numbers">
        <div class="data_element">asdgasf</div>
        <div class="data_element">asdgasf</div>
        <div class="data_element">asdgasf</div>
      </div>
    </div>
    <div id="comment_list">
      <div class="comment">
        <div class="comment_header">
          <div class="profile_image"></div>
          <div class="description">
            <textarea name="offer-description" id="offer-description" cols="1" rows="1"></textarea>
          </div>
        </div>
      </div>
      <div class="comment">
        <div class="comment_header">
          <div class="profile_image"></div>
          <div class="description">
            <textarea name="offer-description" id="offer-description" cols="1" rows="1"></textarea>
          </div>
        </div>
      </div>
    </div>
  </div>
</div>